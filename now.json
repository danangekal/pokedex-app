{
    "version": 2,
    "rewrites": [
      {
        "source": "/service-worker.js",
        "destination": "/_next/static/service-worker.js"
      }
    ],
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=43200, immutable"
          },
          {
            "key": "Service-Worker-Allowed",
            "value": "/"
          }
        ]
      }
    ],
    "env": {
      "BASE_API_URL_EXTERNAL": "https://pokeapi.co/api/v2"
    },
    "build": {
      "env": {
        "BASE_API_URL_EXTERNAL": "https://pokeapi.co/api/v2"
      }
    },
    "builds": [
      {
        "src": "next.config.js",
        "use": "@now/next"
      }
    ]
  }